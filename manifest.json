{
  "manifest_version": 2,

  "name": "Modify href",
  "description": "A Chrome extension for modifying the href of the links on a web page",
  "version": "0.0.1",
  "options_page": "options/options.html",
  "browser_action": {
    "default_icon": "/images/off32.png",
    "default_title": "Click to modify!"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": [
      "/js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "/images/on16.png",
    "32": "/images/on32.png",
    "64": "/images/on64.png",
    "128": "/images/on128.png"
  }
}
